{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "outDir": ".tmp",
    "noEmitOnError": true
  },
  "exclude": [
    "node_modules"
  ],
  "angularCompilerOptions": {
    "genDir": ".aot",
    "skipMetadataEmit": true
  }
}
