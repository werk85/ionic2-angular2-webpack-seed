{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "rules": {
    "arrow-parens": [2, "always"],
    "curly": 0,
    "eol-last": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "max-len": [1, 130],
    "max-nested-callbacks": [1, 4],
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-undef": 2,
    "no-unused-vars": [2, "all"],
    "no-use-before-define": [2, "nofunc"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "strict": 0
  }
}
